{
  "name": "doctor-appointments-express",
  "version": "1.0.0",
  "description": "Doctor Appointment System using Express.js and MongoDB",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest --config ts-jest.config.json --rootDir src",
    "test:watch": "jest --watch --config ts-jest.config.json --rootDir src"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awilix": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^7.4.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "cuid": "3.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@faker-js/faker": "^8.3.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.4.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.6",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
